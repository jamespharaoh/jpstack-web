<plugin
	name="route-core"
	package="wbs.sms.route.core">

	<plugin-dependencies>
		<plugin-dependency name="sms-core"/>
	</plugin-dependencies>

	<models>
		<enum-type name="routeNetworkBehaviour"/>
		<model name="route"/>
	</models>

	<layer name="console">
		<bean class="console.RouteSummaryAdditionalPartManager"/>
		<bean class="console.RouteSummaryAdditionalPart"/>
	</layer>

	<fixtures>
		<fixture name="route"/>
	</fixtures>

	<console-modules>
		<console-module name="route"/>
	</console-modules>

</plugin>
