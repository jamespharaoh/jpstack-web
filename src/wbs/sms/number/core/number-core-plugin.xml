<plugin
	name="number-core"
	package="wbs.sms.number.core">

	<plugin-dependencies>
		<plugin-dependency name="sms-core"/>
	</plugin-dependencies>

	<sql-scripts>
		<sql-schema name="number"/>
	</sql-scripts>

	<models>
		<model name="chatUserNumberReport"/>
		<model name="number"/>
	</models>

	<layer name="logic">
		<component class="logic.NumberLogicImplementation"/>
	</layer>

	<layer name="console">
		<component class="console.NumberConsoleComponents"/>
		<component class="console.NumberLinkManager"/>
		<component class="console.NumberServicesPart"/>
		<component class="console.NumberSubscriptionsPart"/>
	</layer>

	<fixtures>
		<fixture name="number"/>
	</fixtures>

	<console-modules>
		<console-module name="number"/>
	</console-modules>

</plugin>
