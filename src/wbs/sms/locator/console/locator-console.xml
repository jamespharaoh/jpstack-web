<console-module
	name="locator">

	<console-helper-provider
		object-name="locator"
		view-priv="parent">

		<priv-key
			name="locator.manage"
			priv="parent:manage"/>

		<context-stuff
			name="locatorName"
			field="id"/>

	</console-helper-provider>

	<object-context
		name="locator"
		object-name="locator">

		<object>

			<object-summary-page
				form="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="summary"
			object-type="locator"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>

			</columns>

		</form>

	</forms>

</console-module>
